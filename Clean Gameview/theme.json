{
  "name": "Clean Gameview",
  "author": "SuchMeme",
  "version": "v1.2",
  "manifest_version": 2,
  "inject": {
    "shared.css": [
      "SP"
    ]
  },
  "patches": {
    "Show Steam Cloud": {
      "type": "checkbox",
      "default": "No",
      "values": {
        "Yes": {
          "steamcloud.css": [
            "SP"
          ]
        },
        "No": {}
      }
    },
    "Use Blur": {
      "type": "checkbox",
      "default": "No",
      "values": {
        "Yes": {
          "blur.css": [
            "SP"
          ]
        },
        "No": {}
      }
    }
  },
  "id": "a9776f60-2724-4ff0-824b-e1f82a04688e"
}